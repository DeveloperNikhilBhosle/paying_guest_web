@model SK_PG_WebApp.Models.DynamicModels.PayingGuestDC

@{
    ViewData["Title"] = "PayingGuestRoomMapping";
    Layout = "~/Views/Shared/_LayoutUI2.cshtml";
}


            <!-- Horizontal Form -->
            <div class="card card-info" style="margin:20px">
              <div class="card-header">
                <h3 class="card-title">Paying Guest - Room Allocation</h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
              <form class="form-horizontal" method="post">
                <div class="card-body">
                  <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">User Name</label>
                    <div class="col-sm-10">
                      <input type="text" readonly class="form-control" name="userName" value="@ViewBag.userName">
                      <input type="text" readonly style="display:none" class="form-control" name="userId" value="@ViewBag.userId">
                    </div>
                  </div>
                  <div class="form-group row">
                    <label for="inputPassword3" class="col-sm-2 col-form-label">City</label>
                    <div class="col-sm-10">
                        @Html.DropDownList("city",(IEnumerable<SelectListItem>)ViewBag.City , new { 
                                             @class = "form-control", @id = "city", @name = "city" 
                                               
                                         })                   
                    </div>
                  </div>
                  
                </div>
                <!-- /.card-body -->
                <div class="card-footer">
                  <button type="button" class="btn btn-info">Cancel</button>
                  <button type="submit" class="btn btn-default float-right">Next </button>
                </div>
                <!-- /.card-footer -->
              </form>
            </div>
            <!-- /.card -->