@model IEnumerable<SK_PG_WebApp.Models.BusinessModels.PGMasterBO>
@{
    ViewData["Title"] = "Property Dashboard";
    Layout= "_LayoutUI2";
}

            <div class="card card-primary">
              <div class="card-header">
                <h3 class="card-title">New Proprty Floor</h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
              <form method="post">
                <div class="card-body">
                  <div class="form-group">
                    <label for="exampleInputEmail1">Property Details</label>
                    <input type="text" readonly class="form-control" value="@ViewBag.Name">
                    <input type="number" required readonly class="form-control" name="propertyId" value="@ViewBag.id">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">Floor</label>
                    <input type="text" required class="form-control" name="name" placeholder="Floor Name">
                  </div>
                   <div class="form-group">
                    <label for="exampleInputPassword1">Alloated To</label>
                     @Html.DropDownList("ddlPGAllocatedTo",(IEnumerable<SelectListItem>)ViewBag.PGAllocatdTo , new { 
                      @class = "form-control", @id = "pgAllocatedTo", @name = "pgAllocatedTo" 
                       
                  })      
                  </div>
                  
                 
                </div>
                <!-- /.card-body -->

                <div class="card-footer">
                  <button type="submit" class="btn btn-primary">Submit</button>
                </div>
              </form>
            </div>